{
  "extends": [
    "development"
  ],
  "hints": {
    "typescript-config/consistent-casing": "off",
    "typescript-config/strict": "off",
    "disown-opener": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "scrollbar-width",
          "scrollbar-width: none",
          "-webkit-text-size-adjust",
          "field-sizing"
        ]
      }
    ],
    "no-inline-styles": "off"
  }
}